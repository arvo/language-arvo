'scopeName': 'source.arvo'
'name': 'Arvo'
'fileTypes': [
  'arvo'
]
'patterns': [
  {
    'name': 'keyword.control.arvo'
    'match': '\b(fn|module|return)\b'
  }
  {
    'name': 'keyword.operator.arvo'
    'match': '\b(\\+|\\->)\b'
  }
  {
    'name': 'storage.type.arvo'
    'match': '\b(i64)\b'
  }
]
